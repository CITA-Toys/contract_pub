head
  link(ref="manifest" href="/manifest.json")
h1 new contract #{message}

div
  form(action="/contracts/create" method="post")
    textarea#contract(name="bytecode", cols="300", rows="20", placeholder="bytecode" ) #{bytecode}
    textarea#contract(name="interface", cols="300", rows="20", placeholder="interface") #{interface}
    button(type="submit") Submit
